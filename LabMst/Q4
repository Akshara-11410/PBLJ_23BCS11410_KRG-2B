


public class JavaApplication5 {

    public static void main(String[] args) {
       Thread t1 = new Thread(() -> {
            for (int i = 1; i <= 10; i++) {
                System.out.println("Number: " + i);
            }
        });

        Thread t2 = new Thread(() -> {
            for (int i = 1; i <= 10; i++) {
                System.out.println("Square: " + (i * i));
            }
        });

        t1.start();
        t2.start();
    }
    
}
